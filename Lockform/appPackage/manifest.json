{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.17/MicrosoftTeams.schema.json",
    "id": "510661f9-0ef7-4dc1-9d11-916412d5de95",
    "manifestVersion": "1.17",
    "version": "1.0.0",
    "name": {
        "short": "Lockform",
        "full": "Lockform Anti-phishing TaskPane Add-in"
    },
    "description": {
        "short": "Lockform ensures safety in email threads.",
        "full": "Lockform checks the contacts in your email threads against your organization's list of trusted contacts and lets you know in a glance if it is safe to continue with the conversation thread."
    },
    "developer": {
        "name": "Umm Labs",
        "websiteUrl": "https://rentisha.com",
        "privacyUrl": "https://drive.google.com/file/d/1eZ_u0bNIir-g0i9jLqMQ7cPLhGxPVbDU/view?usp=drive_link",
        "termsOfUseUrl": "https://drive.google.com/file/d/1eZ_u0bNIir-g0i9jLqMQ7cPLhGxPVbDU/view?usp=drive_link"
    },
    "icons": {
        "outline": "assets/outline.png",
        "color": "assets/outline.png"
    },
    "accentColor": "#230201",
    "localizationInfo": {
        "defaultLanguageTag": "en-us",
        "additionalLanguages": []
    },
    "authorization": {
        "permissions": {
            "resourceSpecific": [
                {
                    "name": "MailboxItem.Read.User",
                    "type": "Delegated"
                }
            ]
        }
    },
    "validDomains": [
        "rentisha.com"
    ],
    "webApplicationInfo": {
        "id": "5c17ada0-175b-409f-a75c-d076e38274f5",
        "resource": "https://dev.outlook-addin.rentisha.com/"
    },
    "extensions": [
        {
            "requirements": {
                "scopes": [
                    "mail"
                ],
                "capabilities": [
                    {
                        "name": "Mailbox",
                        "minVersion": "1.3"
                    }
                ]
            },
            "runtimes": [
                {
                    "requirements": {
                        "capabilities": [
                            {
                                "name": "Mailbox",
                                "minVersion": "1.3"
                            }
                        ]
                    },
                    "id": "TaskPaneRuntime",
                    "type": "general",
                    "code": {
                        "page": "https://dev.outlook-addin.rentisha.com/"
                    },
                    "lifetime": "short",
                    "actions": [
                        {
                            "id": "TaskPaneRuntimeShow",
                            "type": "openPage",
                            "pinnable": true,
                            "view": "dashboard"
                        }
                    ]
                }
            ],
            "ribbons": [
                {
                    "contexts": [
                        "mailRead"
                    ],
                    "tabs": [
                        {
                            "builtInTabId": "TabDefault",
                            "groups": [
                                {
                                    "id": "msgReadGroup",
                                    "label": "Contoso Add-in",
                                    "icons": [
                                        {
                                            "size": 16,
                                            "url": "https://localhost:4000/assets/icon-16.png"
                                        },
                                        {
                                            "size": 32,
                                            "url": "https://localhost:4000/assets/icon-32.png"
                                        },
                                        {
                                            "size": 80,
                                            "url": "https://localhost:4000/assets/icon-80.png"
                                        }
                                    ],
                                    "controls": [
                                        {
                                            "id": "msgReadOpenPaneButton",
                                            "type": "button",
                                            "label": "Show Taskpane",
                                            "icons": [
                                                {
                                                    "size": 16,
                                                    "url": "https://localhost:4000/assets/icon-16.png"
                                                },
                                                {
                                                    "size": 32,
                                                    "url": "https://localhost:4000/assets/icon-32.png"
                                                },
                                                {
                                                    "size": 80,
                                                    "url": "https://localhost:4000/assets/icon-80.png"
                                                }
                                            ],
                                            "supertip": {
                                                "title": "Show Taskpane",
                                                "description": "Opens a pane displaying all available properties."
                                            },
                                            "actionId": "TaskPaneRuntimeShow"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}